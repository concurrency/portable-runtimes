# Default Arduino Mega configuration: ATmega1280 running at 5V
TVM_MCU=m2560
TVM_GCC_MCU=atmega2560
TVM_BYTECODE_ADDR=0x5500
TVM_F_CPU=16000000
TVM_UPLOAD_RATE=115200
PLATFORM=arduinomega2560
PROGRAMMER=stk500v2
     
# avrdude
TVM_AVRDUDE_FIRMWARE_FLAGS="-V -F -p $TVM_MCU"
TVM_AVRDUDE_CODE_FLAGS="-V -F -p $TVM_MCU -b $TVM_UPLOAD_RATE -c $PROGRAMMER"
TVM_ARDUINO_FIRMWARE=tvm-avr-atmega2560-16000000-arduinomega2560.hex

